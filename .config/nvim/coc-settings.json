{
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "graphql"
  ],
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "yarn",
  "diagnostic.warningSign": "⚠",
  "diagnostic.errorSign": "",
  "diagnostic.hintSign": "⚠",
  "diagnostic.infoSign": "",
  "suggest.snippetIndicator": "►"
}
